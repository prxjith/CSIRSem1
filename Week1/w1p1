#include<stdio.h>
int main()
{
int x,y,a,b,c,d;
printf("Enter two numbers");
scanf("%d",&x);
scanf("%d",&y);
a=x+y;
b=x-y;
c=x*y;
d=x/y;
printf("Result of addition is %d",a);
printf("Result of subtraction is %d",b);
printf("Result of multiplication is %d",c);
printf("Result of division is %d",d);
}

